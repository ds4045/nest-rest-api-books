{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [{ "handle": "filesystem" }, { "src": "/.*", "dest": "/index.js" }],
  "outputDirectory": "dist/apps/nest-rest-api-books"
}
